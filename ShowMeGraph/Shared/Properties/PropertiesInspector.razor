@code {
    private RenderFragment? _inspector;

    [Parameter] public object? InspectedObject { get; set; }

    protected override void OnParametersSet()
    {
        if (InspectedObject is null)
        {
            _inspector = null;
            return;
        }

        var inspectorType = PropertiesInspectorResolver.ResolveType(InspectedObject.GetType());

        if (inspectorType is null)
        {
            _inspector = null;
            return;
        }

        _inspector = RenderInspector(inspectorType);
    }

    private RenderFragment RenderInspector(Type inspectorType) => builder =>
    {
        builder.OpenComponent(0, inspectorType);
        builder.AddAttribute(1, "Model", InspectedObject);
        builder.AddAttribute(2, nameof(OnPropertyChanged), new EventCallback<PropertyValueChangedEventArgs>(this, new Action<PropertyValueChangedEventArgs>(e => OnPropertyChanged.InvokeAsync(e))));
        builder.CloseComponent();
    };
}
